{
  "name": "@txo/react-native-deep-linking",
  "version": "1.0.0",
  "description": "Technology Studio - React native deep linking",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/technology-studio/react-native-deep-linking.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://bitbucket.org/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.technologystudio.sk"
  },
  "scripts": {
    "republish": "npm unpublish $(cat package.json | jq -r '.name')@$(cat package.json | jq -r '.version') && yarn publish --new-version $(cat package.json | jq -r '.version')",
    "build": "yarn build:clean && yarn build:lib && yarn build:flow",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn babel src --out-dir lib --ignore **/__tests__/**",
    "build:flow": "yarn flow-copy-source -i '**/__tests__/**' src lib",
    "prepare": "yarn build",
    "build:watch": "concurrently \"yarn build:watch:lib\" \"yarn build:watch:flow\"",
    "build:watch:lib": "yarn babel src --source-maps --watch --out-dir lib --ignore **/__tests__/**  --verbose",
    "build:watch:flow": "yarn flow-copy-source --verbose -w -i '**/__tests__/**' src lib"
  },
  "dependencies": {
    "@txo/log": "^1.0.0",
    "@txo/react": "^1.0.0",
    "url-parse": "^1.4.7"
  },
  "peerDependencies": {
    "@txo-peer-dep/log": "^3.0.0",
    "@txo-peer-dep/react-conditional-navigation": "^3.0.0",
    "react-native": "*"
  }
}
